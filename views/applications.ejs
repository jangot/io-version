<!DOCTYPE html>
<html>
<head>
<title>IO Versions</title>
<%- include('partial/bootstrapIncludes') %>
</head>

<body>
<%- include('partial/header'); %>

<div class="container-fluid">
    <% for (app of applications) { %>
        <h3>
            <a href="/applications/<%= app.name %>"><%= app.name %></a>
        </h3>
        <ul>
            <% for (version of app.versions) { %>
                <li><%= version.version %></li>
            <% } %>
        </ul>
    <% } %>
</div>

<%- include('partial/footer') %>
</body>

</html>